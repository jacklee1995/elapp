{
  "name": "@elapp/build",
  "version": "1.0.0",
  "description": "",
  "main": "./src/main.js",
  "scripts": {
    "serve": "electron .",
    "build": "electron-builder"
  },
  "keywords": [],
  "author": {
    "name": "jcLee1995",
    "email": "291148484@163.com",
    "url": "https://github.com/jacklee1995"
  },
  "homepage": "https://github.com/jacklee1995/elapp",
  "license": "MIT",
  "dependencies": {
    "@electron/asar": "^3.2.2",
    "@electron/osx-sign": "^1.0.1",
    "@electron/remote": "^2.0.8",
    "@vueuse/core": "^9.6.0",
    "log4js": "^6.7.1",
    "node-gyp": "^9.3.0",
  
    "electron-is": "^3.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^5.3.0",
    "date-format": "^4.0.14",
    "debug": "^4.3.4",
    "flatted": "^3.2.7",
    "fs-extra": "^11.1.0",
    "get-port": "^6.1.2",
    "graceful-fs": "^4.2.10",
    "jsonfile": "^6.1.0",
    "ms": "^2.1.3",
    "rfdc": "^1.3.0",
    "streamroller": "^3.1.3",
    "universalify": "^2.0.0"
  },
  "devDependencies": {
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "@electron/rebuild": "^3.2.10",
    "@npmcli/fs": "^3.1.0",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "wait-on": "^6.0.1"
  },
  "build": {
    "appId": "tech.thispage.el-app",
    "productName": "el-app",
    "copyright": "Copyright Â© 2022 jcLee1995",
    "asar": true,
    "win": {
      "icon": "./assets/favicon_256x256.ico",
      "asar": true,
      "requestedExecutionLevel": "requireAdministrator",
      "compression": "store",
      "executableName": "elapp",
      "fileAssociations": [
        {
          "ext": ".md",
          "name": ".MD ext",
          "description": "MarkDown File (el-app)",
          "icon": "./assets/file-markdown_256x256.ico"
        },
        {
          "ext": ".txt",
          "name": ".TXT ext",
          "description": "TEXT File (el-app)",
          "icon": "./assets/file-text_256x256.ico"
        }
      ],
      "extraFiles": [
        {
          "from": "./sources",
          "to": "sources"
        }
      ],
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "./assets/*",
      "./src/**/*",
      "./renders/**/*",
      "./sources/**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}"
    ],
    "icon": "./assets/jcstudio_256x256.ico",
    "directories": {
      "buildResources": "assets",
      "output": "release"
    }
  }
}
