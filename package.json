{
  "name": "electron-vite-ts-app",
  "version": "0.0.1",
  "main": "./dist/main.js",
  "author": {
    "name": "jcLee1995",
    "email": "291148484@163.com",
    "url": "https://github.com/jacklee1995"
  },
  "license": "MIT",
  "repository": "https://github.com/jacklee1995/electron-vite-ts-app",
  "description": "A Electron-Vite-Vue  application written with Typescript.",
  "scripts": {
    "install": "pwsh -Command \"[System.Environment]::SetEnvironmentVariable('NODE_ENV','development')\" && npm install",
    "vite:serve": "pwsh -Command \"[System.Environment]::SetEnvironmentVariable('NODE_ENV','development')\" && vite --host",
    "vite:build": "pwsh -Command \"[System.Environment]::SetEnvironmentVariable('NODE_ENV','development')\" && vite build",
    "electron": "pwsh -Command \"[System.Environment]::SetEnvironmentVariable('NODE_ENV','development')\" && tsc && electron ./dist/main.js",
    "electron:serve": "pwsh -Command \"[System.Environment]::SetEnvironmentVariable('NODE_ENV','development')\" && concurrently -k \"npm run vite:serve\" \"npm run electron\"",
    "serve": "nodemon -e ts --watch electron --exec \"npm run electron\"",
    "build": "pwsh -Command \"[System.Environment]::SetEnvironmentVariable('NODE_ENV','production')\" && vite build && electron-builder"
  },
  "dependencies": {
    "vue": "^3.2.41",
    "@electron/asar": "^3.2.2",
    "@electron/osx-sign": "^1.0.1",
    "@electron/remote": "^2.0.8",
    "@vueuse/core": "^9.3.1",
    "log4js": "^6.7.0",
    "node-gyp": "^9.3.0",
    "pinia": "^2.0.21",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@npmcli/fs": "^3.1.0",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vue/tsconfig": "^0.1.3",
    "concurrently": "^7.5.0",
    "cross-env": "^7.0.3",
    "electron": "^21.3.0",
    "electron-builder": "^23.6.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "sass": "^1.56.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vue-tsc": "^1.0.9",
    "wait-on": "^6.0.1"
  },
  "private": true,
  "build": {
    "appId": "tech.thispage.elects-vite",
    "productName": "elects-vite",
    "copyright": "Copyright Â© 2022 jcLee1995",
    "win": {
      "icon": "./public/favicon_256x256.ico",
      "asar": true,
      "requestedExecutionLevel": "requireAdministrator",
      "compression": "store",
      "executableName": "demo",
      "extraFiles": "",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "asar": true,
    "icon": "./public/jcstudio_256x256.ico",
    "extraFiles": [
      {
        "from": "resources",
        "to": "resources"
      },
      {
        "from": "about",
        "to": "about"
      }
    ],
    "directories": {
      "buildResources": "assets",
      "output": "release"
    }
  }
}
